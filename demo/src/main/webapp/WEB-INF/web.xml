<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         <welcome-file-list>
         <welcome-file>index.jsp</welcome-file>
         </welcome-file-list>
         
         <servlet>
         <servlet-name>login</servlet-name>
         <servlet-class>com.te.login.Insert</servlet-class>
         </servlet>
         <servlet-mapping>
         <servlet-name>login</servlet-name>
         <url-pattern>/login</url-pattern>
         </servlet-mapping>
         <servlet>
         <servlet-name>verify</servlet-name>
         <servlet-class>com.te.login.Verification</servlet-class>
         </servlet>
         
         <servlet-mapping>  
         <servlet-name>verify</servlet-name>
         <url-pattern>/verification</url-pattern>
         </servlet-mapping>
         <servlet>
         <servlet-name>invalid</servlet-name>
         <servlet-class>com.te.login.Invalid</servlet-class>
         </servlet>
         <servlet-mapping>
         <servlet-name>invalid</servlet-name>
         <url-pattern>/invalid</url-pattern>
         </servlet-mapping>
        
         
         </web-app>
         